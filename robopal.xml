<?xml version='1.0' encoding='UTF-8' standalone='yes' ?>
<Robot type="FirstInspires-FTC">
    <DeviceInterfaceModule name="Device Interface Module" serialNumber="AL00VFFN">
        <AdafruitBNO055IMU name="imu" port="0" />
        <I2cDevice name="mux" port="1" />
        <I2cDevice name="color sensor" port="2" />
    </DeviceInterfaceModule>
    <MotorController name="Left Motor Controller" serialNumber="AL00VEA0">
        <NeveRest40Gearmotor name="bl" port="1" />
        <NeveRest40Gearmotor name="fl" port="2" />
    </MotorController>
    <ServoController name="Arm Servo Controller" serialNumber="AI02QSKO">
        <ContinuousRotationServo name="relicExtendArm1" port="6" />
        <ContinuousRotationServo name="relicExtendArm2" port="5" />
        <ContinuousRotationServo name="relicElbow" port="4" />
        <Servo name="relicGrab" port="3" />
        <Servo name="relicWrist" port="2" />
    </ServoController>
    <MotorController name="Lift Motor Controller" serialNumber="AL00YCWT">
        <NeveRest40Gearmotor name="lift" port="1" />
    </MotorController>
    <ServoController name="Jewel Servo Controller" serialNumber="AI02QTIN">
        <Servo name="jewelServoBlue" port="5" />
        <Servo name="jewelServoRed" port="6" />
        <Servo name="glyphGrab" port="4" />
    </ServoController>
    <MotorController name="Right Motor Controller" serialNumber="AL00VE17">
        <NeveRest40Gearmotor name="br" port="1" />
        <NeveRest40Gearmotor name="fr" port="2" />
    </MotorController>
</Robot>
